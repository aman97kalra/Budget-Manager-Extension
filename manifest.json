{
    "manifest_version":2,
    "name": "Budget Manager",
    "version": "1.0",
    "description": "An extension to keep track of budget",
    "browser_action":{
        "default_icon":"get_started16.png",
        "default_popup":"popup.html"
    },
    "options_page":"options.html",
    "background":{
        "scripts":  ["eventPage.js"],
        "persistent":   false
    },
    "permissions":[
        "storage",
        "notifications",
        "contextMenus"
    ]
}